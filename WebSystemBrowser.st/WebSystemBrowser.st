WAComponent subclass: #WebClassBrowserComponent	instanceVariableNames: 'subjectClass'	classVariableNames: ''	poolDictionaries: ''	category: 'WebSystemBrowser'!!WebClassBrowserComponent commentStamp: 'Casco 3/25/2019 13:57' prior: 0!I am a class browser. I know a class that I browse (my subjectClass)!!WebClassBrowserComponent methodsFor: 'accessing' stamp: 'Casco 3/25/2019 13:52'!subjectClass	^ subjectClass! !!WebClassBrowserComponent methodsFor: 'accessing' stamp: 'Casco 3/25/2019 13:52'!subjectClass: aClass	subjectClass := aClass! !!WebClassBrowserComponent methodsFor: 'rendering' stamp: 'Casco 3/25/2019 13:58'!renderContentOn: anHtmlCanvas	anHtmlCanvas heading		level: 1;		with: subjectClass name.	anHtmlCanvas heading		level: 2;		with: 'Commentario'.	anHtmlCanvas paragraph: subjectClass comment.	anHtmlCanvas heading		level: 2;		with: 'Métodos'.	anHtmlCanvas unorderedList with: [ subjectClass selectors do: [ :each | anHtmlCanvas listItem with: each ] ]! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!WebClassBrowserComponent class	instanceVariableNames: ''!!WebClassBrowserComponent class methodsFor: 'instance-creation' stamp: 'Casco 3/25/2019 13:55'!subjectClass: aClass	^ self new		subjectClass: aClass;		yourself! !WAComponent subclass: #WebSystemBrowserComponent	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'WebSystemBrowser'!!WebSystemBrowserComponent commentStamp: 'Casco 3/25/2019 13:22' prior: 0!WAAdmin register: WebSystemBrowserComponent asApplicationAt: 'systemBrowser'!!WebSystemBrowserComponent methodsFor: 'callbacks' stamp: 'Casco 3/25/2019 13:56'!browseClass: aClass	self show: (WebClassBrowserComponent subjectClass: aClass)! !!WebSystemBrowserComponent methodsFor: 'rendering' stamp: 'Casco 3/25/2019 13:58'!renderContentOn: anHtmlCanvas	anHtmlCanvas heading: 'Subclases de WABrush'.	anHtmlCanvas paragraph: 'Para ir aprendiendo sobre los pinceles con los que se pinta en un WAHtmlCanvas'.	anHtmlCanvas unorderedList		with: [ WABrush allSubclasses				do: [ :each | 					anHtmlCanvas listItem						with: [ anHtmlCanvas anchor								callback: [ self browseClass: each ];								with: each name ] ] ]! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!WebSystemBrowserComponent class	instanceVariableNames: ''!!WebSystemBrowserComponent class methodsFor: 'initialization' stamp: 'Casco 3/25/2019 14:01'!initialize	super initialize.	WAAdmin register: WebSystemBrowserComponent asApplicationAt: 'WebSystemBrowser'! !WebSystemBrowserComponent initialize!